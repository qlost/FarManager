NAME = FarCmds
NOENTRY=1
NODEFLIB=1
INCLUDECRT=1
USERCPP=/W3 /analyze- /WX- /Gs10000 /GS- /GR- /EHs-c- /D_HAS_EXCEPTIONS=0
!ifndef DEBUG
USERCPP=$(USERCPP) /O1 /Oi /Os /Oy
!else
USERCPP=$(USERCPP) /DDEBUG /Od
!endif
ENV_LIB_OPT=/merge:.pdata=.rdata /safeseh:no /debug:none

LINK_OBJS = \
	$(INTDIR)\FARCmds.obj \
	$(INTDIR)\Mix.obj \
	$(INTDIR)\OpenCmd.obj

USERLIBS = rpcrt4.lib uuid.lib

!include ..\makefile_vc_def_inc
!include ..\makefile_vc_target_inc

$(INTDIR)\FARCmds.obj: FARCmds.cpp FARCmds.hpp Lang.hpp guid.hpp version.hpp $(COMINC)\plugin.hpp $(COMINC)\PluginSettings.hpp $(COMINC)\DlgBuilder.hpp
$(INTDIR)\Mix.obj: Mix.cpp FARCmds.hpp Lang.hpp version.hpp guid.hpp $(COMINC)\plugin.hpp
$(INTDIR)\OpenCmd.obj: OpenCmd.cpp FARCmds.hpp Lang.hpp version.hpp guid.hpp $(COMINC)\plugin.hpp
